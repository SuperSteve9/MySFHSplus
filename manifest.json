{
    "manifest_version": 3,
    "name": "MySFHS+",
    "version": "2.5.10",
    "description": "Adds many features to the mySFHS website",
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "img/128.png"
    },
    "content_scripts": [
    {
        "matches": ["https://sfhscollegeprep.myschoolapp.com/*"],
        "js": ["content.js"],
        "run_at": "document_idle",
        "type": "module"
    }
    ],
    "permissions": ["scripting", "activeTab"]
}